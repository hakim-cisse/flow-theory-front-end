{
  "rewrites": [
    {
      "source": "/blog/:slug",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|WhatsApp|Slackbot|TelegramBot|Discordbot|Pinterest|Googlebot|bingbot).*"
        }
      ],
      "destination": "https://gkwsgbldtojarjsulbof.supabase.co/functions/v1/og-redirect?slug=:slug"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
